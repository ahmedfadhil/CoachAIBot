    <div class="container">
      <div class="row active-with-click">
        <div class="col-md-4 col-sm-6 col-xs-12 card_height">
          <article class="material-card Red">
            <h2>
          <span class="card_center"> <%= user.last_name %> <%= " #{user.first_name}" %>
</span>
              <%= link_to features_users_path(user), class: 'btn btn-info' do %>
                Vedi utente
              <% end %>
              <strong>
                <a class="text_color"><i class="material-icons">date_range</i>
                  <%= user.created_at.strftime("%d %b. %Y") %></a>
              </strong>

              <div class="col-sm-3 user_view_buttons three_bottons">

                <% if archived? %>
                  <div class="row">
                    <div class="user-card-n">

                      <a href="<%= restore_users_path(user) %>">
                        <%= image_tag 'restore_user_icon.png', class: 'restore-user' %>
                      </a>
                    </div>
                  </div>


                <% else %>
                  <div class="row">
                    <div class="user-card-n">
                      <a href="<%= chat_chats_path(user) %>">
                        <i class="chat-with-user n-icon mdl-color-text--blue-grey-400 material-icons" role="presentation">question_answer</i>
                      </a>
                    </div>
                  </div>
                  <div class="row">
                    <div class="user-card-n">
                      <a href="<%= archive_users_path(user) %>">

                        <i class="material-icons archive-user" role="presentation">theaters</i>

                      </a>
                    </div>
                  </div>
                  <% unless user.profiled? %>
                    <div class="row">
                      <div class="user-card-n">
                        <%= link_to user_path(user), method: :delete, data: {confirm: 'Sei sicuro di voler eliminare l\'utente? Una volta eliminato non potra\' piu\' essere recuperato.'} do %>
                          <i class="delete-user n-icon mdl-color-text--blue-grey-400 material-icons" role="presentation">delete</i>
                        <% end %>
                      </div>
                    </div>
                  <% end %>


                <% end %>


              </div>
            </h2>

            <div class="mc-content">
              <div class="img-container">
                <div class="img-responsive" id="profile-img-user-<%= user.id %>">

                </div>
              </div>
              <div class="mc-description">
                <!--He has appeared in more than 100 films and television shows, including The Deer Hunter, Annie Hall, The-->
                <!--Prophecy trilogy, The Dogs of War ...-->
              </div>


            </div>

            <a class="mc-btn-action">
              <i class="fa fa-bars"></i>
            </a>
            <div class="mc-footer">
              <% unless archived? %>
                <div class="scores-grid mdl-grid three_adherence">
                  <div class="col-sm-4">
                    <div class="score" id="physical_score_<%= user.id %>">
                      <h4>
                        Esercizio
                      </h4>
                    </div>
                  </div>
                  <div class="col-sm-4">
                    <div class="score" id="diet_score_<%= user.id %>">
                      <h4>
                        Dieta
                      </h4>
                    </div>
                  </div>
                  <div class="col-sm-4">
                    <div class="score" id="mental_score_<%= user.id %>">
                      <h4>
                        Mentale
                      </h4>
                    </div>
                  </div>

                </div>
              <% end %>


            </div>

          </article>

        </div>

      </div>

