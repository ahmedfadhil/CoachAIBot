<div class="mdl-layout mdl-js-layout mdl-layout--fixed-header">
  <header class="mdl-layout__header">
    <div class="mdl-layout__header-row">
      <span class="mdl-layout-title">CoachAI - Admin</span>
      <div class="mdl-layout-spacer"></div>

    </div>

    <!--<div class="mdl-layout__tab-bar mdl-js-ripple-effect">-->
    <!--<a href="#fixed-tab-1" class="mdl-layout__tab is-active">About</a>-->
    <!--<a href="#fixed-tab-2" class="mdl-layout__tab">Learn</a>-->
    <!--</div>-->
  </header>

  <div class="mdl-layout__drawer">

    <div class="avatar">
      <%= image_tag coach.avatar.url, :class => "avatar", :alt => "Kaptain Kitty" %>
    </div>
    <span class="mdl-layout-title"><%= coach.first_name %> <%= coach.last_name %></span>
    <!-- /.avatar -->

    <div class="drawer-text container">
      Il pannello di amministrazione per CoachAI
    </div>
    <!-- /.drawer-text -->

    <nav class="mdl-navigation">
      <hr width="50%" style="border-style: dotted;">
      <div class="col-xs-12">
        <%= link_to profile_index_path, class: 'mdl-navigation__link' do %>
          <i class="material-icons"> dashboard</i> DASHBOARD
        <% end %>
      </div>
      <hr width="50%" style="border-style: dotted;">

      <div class="col-xs-12">
        <%= link_to users_path, class: 'mdl-navigation__link' do %>
          <i class="material-icons">people</i>PAZIENTI
        <% end %>
      </div>
      <hr width="50%" style="border-style: dotted;">
      <div class="col-xs-12">
        <%= link_to activities_path, class: 'mdl-navigation__link' do %>
          <i class="material-icons">library_books</i> ATTIVITÀ
        <% end %>
      </div>
      <hr width="50%" style="border-style: dotted;">

      <div class="col-xs-12">
        <%= link_to "#", class: 'mdl-navigation__link' do %>
          <i class="material-icons">devices_other</i> INDOSSABILE
        <% end %>
      </div>
      <hr width="50%" style="border-style: dotted;">
      <div class="col-xs-12">
        <%= link_to "#", class: 'mdl-navigation__link' do %>
          <i class="material-icons">nature_people</i> APPUNTAMENTI
        <% end %>
      </div>
      <hr width="50%" style="border-style: dotted;">
      <div class="col-xs-12">
        <%= link_to "#", class: 'mdl-navigation__link' do %>
          <i class="material-icons">forum</i> DIALOGO
        <% end %>
      </div>
      <hr width="50%" style="border-style: dotted;">
      <div class="col-xs-12">
        <%= link_to root_path, class: 'mdl-navigation__link' do %>
          <i class="material-icons">home</i> HOME
        <% end %>
      </div>
      <hr width="50%" style="border-style: dotted;">


    </nav>


