                            <div class="user-card-n">
                              <a href="<%= archive_users_path(red_user) %>">

                                <i class="material-icons archive-user" role="presentation">theaters</i>

                              </a>
                            </div>
                          </div>

                          <!--<button class="btn btn-warning btn-fab btn-round">-->
                          <!--<#%= plans_count(yellow_user) %>-->
                          <!--</button>-->
                          <!---->
                          <!--<button class="btn btn-warning btn-fab btn-round">-->
                          <!--<#%= activities_count(yellow_user) %>-->
                          <!--</button>-->

                        <% end %>
                        </div>
                  </div>

                <% end %>



                <% yellows.each do |yellow_user| %>


                  <div class="media user-follower">
                    <%= image_tag user_profile_image(yellow_user), alt: 'paziente', class: ' media-object pull-left' %>
                    <div class="media-body">
                      <a><%= user_name_link(yellow_user) %><br>
                        <% if yellow_user.patient_objective == nil %>
