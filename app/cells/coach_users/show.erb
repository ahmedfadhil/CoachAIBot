                            <i class="fa fa-checkmark-round"></i> <span>sdadasdTO</span></button>

                        <% end %>

                        <% if red_user %>

                          <button type="button" class="btn btn-danger btn-round btn-xs pull-right">
                            <i class="material-icons md-40">sentiment_very_dissatisfied</i></button>

                          <!--<button type="button" class="btn btn-sm btn-toggle-following pull-right">-->
                          <!--<i class="fa fa-checkmark-round"></i> <span>STATO</span></button>-->


