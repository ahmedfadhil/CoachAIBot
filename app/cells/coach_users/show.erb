                          </span></a>

                        <% end %>

                        <% if yellow_user %>

                          <button type="button" class="btn btn-warning btn-round btn-xs pull-right">
                            <i class="material-icons md-40">sentiment_neutral</i></button>


                          <!--to archive users-->
                          <!--<button type="button" class="btn btn-sm btn-toggle-following pull-right">-->
                          <!--<i class="fa fa-checkmark-round"></i> <span>STATO</span></button>-->


                          <div class="row">
                            <div class="user-card-n">
                              <a href="<%= archive_users_path(yellow_user) %>">

                                <i class="material-icons archive-user" role="presentation">theaters</i>

                              </a>
                            </div>
                          </div>

                          <!--<button class="btn btn-warning btn-fab btn-round">-->
                          <!--<#%= plans_count(yellow_user) %>-->
                          <!--</button>-->
                          <!---->
                          <!--<button class="btn btn-warning btn-fab btn-round">-->
                          <!--<#%= activities_count(yellow_user) %>-->
                          <!--</button>-->

                        <% end %>
                        </div>
                  </div>

                <% end %>
                <% greens.each do |green_user| %>


                  <div class="media user-follower">
                    <%= image_tag user_profile_image(green_user), alt: 'paziente', class: ' media-object pull-left' %>
                    <div class="media-body">
                      <a><%= user_name_link(green_user) %><br>
                        <% if green_user.patient_objective == nil %>
                      <span class="text-muted username">
