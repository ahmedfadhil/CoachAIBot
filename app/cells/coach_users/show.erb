                <% if green_user.nil? %>
                  <p><span class="label label-success">0</span> PAZIENTI ATTIVI</p>
                <% else %>
                  <p><span class="label label-success"><%= green_count %></span> PAZIENTI ATTIVI</p>
                <% end %>
              <% end %>

              <% yellows.each do |yellow_user| %>
