                          <button type="button" class="btn btn-success btn-round btn-xs pull-right">
                            <i class="material-icons md-40">sentiment_very_satisfied</i></button>

                          <!--<button type="button" class="btn btn-sm btn-toggle-following pull-right">-->
                          <!--<i class="fa fa-checkmark-round"></i> <span>STATO</span></button>-->


                          <div class="row">
                            <div class="user-card-n">
                              <a href="<%= archive_users_path(green_user) %>">

