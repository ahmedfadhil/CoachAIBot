    <!--<button id="btn_login_1" class="banner-btn mdl-button mdl-js-button mdl-button&#45;&#45;raised mdl-js-ripple-effect large-btn-home">-->
    <a id="btn_login_1" class="btn btn-success btn-large raised mdl-js-ripple-effect nav-tabs-navigation "
       data-toggle="tab"><i
    class="material-icons">chat</i> Login </a>

    <!--</button>-->
    <!-- Element to pop up -->
    <div id="login_pop_up">
      <%= render 'home/login_form' %>
    </div>

  <% end %>


</div>


