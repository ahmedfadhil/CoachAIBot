<% provide(:title, 'Home') %>

<div class="banner">

</div>
<div class="btn-banner">
  <% if coach_user_signed_in? %>
    <%= link_to profile_index_path, class: 'banner-btn mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect large-btn-home' do %>
      Dashboard
    <% end %>
  <% else %>
